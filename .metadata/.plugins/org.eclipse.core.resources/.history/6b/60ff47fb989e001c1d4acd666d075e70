package com.riosoft.enumerados;

/**
 * Enumerado para los estados l√≥gicos de registro de una tabla
 * @author Oljer Cando
 *
 */
public class EstadoRegistro {
    
    ACTIVO(Boolean.TRUE),
    ELIMINADO(Boolean.FALSE);

    private final Boolean estado;

    EstadoRegistro(Boolean estado) {
        this.estado = estado;
    }

    public Boolean getEstado() {
        return this.estado;
    }

}
